    "import requests\n",
    "import json\n",
    "\n",
    "\n",
    "url = \"https://api.upbit.com/v1/ticker\"\n",
    "\n",
    "querystring = {\"markets\":\"KRW-XRP\"}\n",
    "\n",
    "response = requests.request(\"GET\", url, params=querystring)\n",
    "\n",
    "res_json = response.json()\n",
    "\n",
    "a = str(res_json[0]['trade_price'])\n",
    "\n",
    "print(\"리플 현재가 \" + a)"
   ]
