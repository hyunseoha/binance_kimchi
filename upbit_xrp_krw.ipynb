    "import requests\n",
    "from pyquery import PyQuery as pq\n",
    "import json\n",
    "\n",
    "data_text = pq(url='https://api.upbit.com/v1/ticker?markets=KRW-XRP')\n",
    "data_json = json.loads(data_text.text())\n",
    "\n",
    "type(data_json)\n",
    "print(data_json[0]['trade_price'])\n",
    "\n"
   ]
  },
